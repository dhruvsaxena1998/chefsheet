{
  "kind": "collectionType",
  "collectionName": "clients",
  "info": {
    "singularName": "client",
    "pluralName": "clients",
    "displayName": "Clients",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "contact_number": {
      "type": "string"
    },
    "country_code": {
      "type": "string",
      "default": "+91"
    },
    "settings": {
      "type": "json"
    },
    "manager": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "clients"
    },
    "address": {
      "type": "component",
      "repeatable": false,
      "component": "shared.address"
    },
    "events": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event.event",
      "mappedBy": "client"
    }
  }
}
